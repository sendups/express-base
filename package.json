{
  "name": "yasui",
  "version": "0.2.4",
  "description": "Light framework using Express for Node.js applications",
  "main": "./lib/index.js",
  "type": "module",
  "scripts": {
    "example": "npm run build && node lib/examples/app.js",
    "build": "tsc && npm run types-build",
    "types-build": "cd src/types/ && tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "preversion": "npm run lint"
  },
  "files": [
    "lib/**/*",
    "!lib/examples/**/*",
    "@types/**/*",
    "!@types/enums.d.ts"
  ],
  "typings": "./@types/index.d.ts",
  "author": "Sendups",
  "keywords": [
    "express",
    "framework",
    "base",
    "light",
    "minimalist"
  ],
  "license": "ISC",
  "dependencies": {
    "kleur": "^4.1.3",
    "express": "^4.17.1",
    "mongoose": "^5.4.13",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "source-map-support": "^0.5.19",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sendups/yasui.git"
  },
  "bugs": {
    "url": "https://github.com/sendups/yasui/issues"
  },
  "homepage": "https://github.com/sendups/yasui#readme"
}
